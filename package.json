	"license": "MIT",
	"bugs": {
		"url": "https://github.com/arjunpat/bell-countdown/issues"
	},
	"homepage": "https://github.com/arjunpat/bell-countdown#readme",
	"dependencies": {
		"dotenv": "^6.0.0",
		"mysql": "^2.16.0"
	},
	"devDependencies": {
		"nodemon": "^1.18.3",
		"webpack": "^4.17.1",
		"webpack-cli": "^3.1.0"
	}
}
