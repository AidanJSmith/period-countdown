	"dependencies": {
		"dotenv": "^6.0.0",
		"mysql": "^2.16.0"
	},
	"devDependencies": {
		"@babel/core": "^7.0.0",
		"@babel/polyfill": "^7.0.0",
		"@babel/preset-env": "^7.0.0",
		"babel-loader": "^8.0.2",
		"nodemon": "^1.18.4",
